<Project Sdk="Microsoft.NET.Sdk.BlazorWebAssembly">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly" Version="6.0.0" />
    <PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly.DevServer" Version="6.0.0" PrivateAssets="all" />
    <PackageReference Include="PublishSPAforGitHubPages.Build" Version="1.3.2" />
    <PackageReference Include="Skclusive.Material.Component" Version="5.2.0" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="PublishSPAforGitHubPages.Build" />
    <None Remove="Skclusive.Material.Menu" />
    <None Remove="Skclusive.Material.Component" />
    <None Remove="AntDesign" />
    <None Remove="Pages\RoutineTree\" />
    <None Remove="Pages\Apps\RoutineTree\" />
    <None Remove="Pages\Parts\" />
  </ItemGroup>
  <ItemGroup>
    <Folder Include="wwwroot\images\" />
    <Folder Include="Pages\Parts\" />
  </ItemGroup>
  <ItemGroup>
    <None Update="Pages\image.png">
      <ExcludeFromSingleFile>true</ExcludeFromSingleFile>
    </None>
  </ItemGroup>
  <ItemGroup>
    <Content Remove="wwwroot\images\" />
    <Content Remove="wwwroot\images\image.png" />
  </ItemGroup>
</Project>
